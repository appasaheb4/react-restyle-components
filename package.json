{
  "name": "react-restyle-components",
  "version": "0.1.56",
  "private": false,
  "description": "Easy use restyle components",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "author": {
    "email": "onlyappasaheb4@gmail.com",
    "name": "Lakade Appasaheb",
    "url": "http://github.com/appasaheb4"
  },
  "license": "MIT",
  "scripts": {
    "start": "serve -s ./build",
    "dev": "chmod +x setup.sh && sh ./setup.sh && craco start",
    "serve-webapp": "craco build && craco start",
    "build": "rm -rf lib && chmod +x npm-publish.sh && sh ./npm-publish.sh && tsc -p tsconfig.json  & tsc -p tsconfig-cjs.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "precommit": "lint-staged --allow-empty",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "rm -rf docs-build && storybook build -o docs-build",
    "deploy-storybook": "gh-pages -d docs-build",
    "prettier": "prettier --write '**/*.{js,jsx}'",
    "go": "npm run build && npm publish"
  },
  "dependencies": {
    "@loadable/component": "^5.16.4",
    "@mui/material": "^5.15.19",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "dayjs": "^1.11.1",
    "react-datepicker": "^4.7.0",
    "react-icons": "^5.2.1",
    "reactstrap": "^9.0.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "@types/jest": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "dayjs": "*",
    "react": "*",
    "react-datepicker": "*",
    "react-dom": "*",
    "react-scripts": "*",
    "reactstrap": "*",
    "tailwindcss": "*",
    "typescript": "*",
    "web-vitals": "*"
  },
  "files": [
    "*.md",
    "lib",
    "*.css"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.1",
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.0.6",
    "@storybook/addon-interactions": "^8.0.6",
    "@storybook/addon-links": "^8.0.6",
    "@storybook/addon-onboarding": "^8.0.6",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.6",
    "@storybook/react": "^8.0.6",
    "@storybook/react-webpack5": "^8.0.6",
    "@storybook/test": "^8.0.6",
    "@svgr/webpack": "^8.1.0",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "craco-alias": "^3.0.1",
    "css-loader": "^7.1.2",
    "eslint-plugin-storybook": "^0.8.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "postcss-loader": "^8.1.1",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "storybook": "^8.0.6",
    "style-loader": "^4.0.0",
    "webpack": "^5.72.0"
  },
  "keywords": [
    "react",
    "components",
    "restyle-components",
    "tech-abl"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appasaheb4/react-restyle-components.git"
  },
  "bugs": {
    "url": "https://github.com/appasaheb4/react-restyle-components/issues"
  },
  "homepage": "https://github.com/appasaheb4/react-restyle-components#readme",
  "lint-staged": {
    "*.{js,jsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
