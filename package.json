{
  "name": "react-restyle-components",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.113",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "serve -s ./build",
    "dev": "craco start",
    "serve-webapp": "craco build && craco start",
    "build": "rm -rf lib && chmod +x npm-publish.sh && sh ./npm-publish.sh && tsc -p tsconfig.json  & tsc -p tsconfig-cjs.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "precommit": "lint-staged --allow-empty",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "rm -rf docs-build && storybook build -o docs-build",
    "deploy-storybook": "gh-pages -d docs-build",
    "prettier": "prettier --write '**/*.{js,jsx}'",
    "go": "npm run build && npm publish"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
